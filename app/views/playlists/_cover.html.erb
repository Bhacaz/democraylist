<div class="playlist-cove-tile p-2 ">
  <%= link_to "/playlists/#{playlist.id}" do %>
    <div class="box">
      <div class="block">
        <%= image_tag playlist.image_url, sizes: '8em' %>
      </div>
      <div class="block">
        <h6 class="title is-6 playlist-name"><%= playlist.name %></h6>
      </div>
    </div>
  <% end %>
</div>
